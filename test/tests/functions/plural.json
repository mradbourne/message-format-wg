{
  "scenario": "Plural functions",
  "defaultTestParams": {
    "testType": "function",
    "locale": "en-US"
  },
  "tests": [
    {
      "src": ".match {$foo :plural} one {{one}} * {{other}}",
      "params": [
        {
          "type": "number",
          "name": "foo",
          "value": 1
        }
      ],
      "exp": "other",
      "expErrors": [
        {
          "type": "missing-func"
        },
        {
          "type": "not-selectable"
        }
      ]
    },
    {
      "src": "hello {42 :plural}",
      "exp": "hello {|42|}",
      "expErrors": [
        {
          "type": "missing-func"
        }
      ]
    }
  ]
}
