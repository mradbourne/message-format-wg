{
  "scenario": "Ordinal functions",
  "defaultTestParams": {
    "testType": "function",
    "locale": "en-US"
  },
  "tests": [
    {
      "src": ".match {$foo :ordinal} one {{st}} two {{nd}} few {{rd}} * {{th}}",
      "params": [
        {
          "type": "number",
          "name": "foo",
          "value": 1
        }
      ],
      "exp": "th",
      "expErrors": [
        {
          "type": "missing-func"
        },
        {
          "type": "not-selectable"
        }
      ]
    },
    {
      "src": "hello {42 :ordinal}",
      "exp": "hello {|42|}",
      "expErrors": [
        {
          "type": "missing-func"
        }
      ]
    }
  ]
}
